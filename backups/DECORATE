ACTOR Anemone01 15000
{
	//$Category Models/Plants
	//$Anemone01
	//$Color 3
	Radius 12
	Height 24
	Mass 10
	+SOLID
	+SHOOTABLE
	+DONTTHRUST
	+NOBLOOD
	+NOGRAVITY
	States
	{
	Spawn:
		MDLA A -1
		Stop
	}
}

